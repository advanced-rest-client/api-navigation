#%RAML 1.0 Library

types:
  myType:
    properties:
      foo: boolean
      bar: string
resourceTypes:
  myResourceType:
    get:
      headers:
        x-header: string
traits:
  myTrait:
    queryParameters:
      abc:
        type: string
        description: test type
        default: yes
        minLength: 1
        maxLength: 3
    headers:
      x-abc:
        type: string
        default: no
        pattern: (yes|no)
    securedBy: [basic, null]
    responses:
      200:
        description: test response
        headers:
          X-Frame-Options:
            type: string
securitySchemes:
  basic:
    type: Basic Authentication
    settings:
      x-custom: string
